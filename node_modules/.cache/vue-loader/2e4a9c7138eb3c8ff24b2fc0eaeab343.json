ng\">\n              <star-rating\n                :rating=\"course.avgRating\"\n                :increment=\"0.1\"\n                :star-size=\"18\"\n                :read-only=\"true\"\n              ></star-rating>\n            </div>\n            <div class=\"course-card__price\">\n              <div class=\"course-card__price--origin\">{{course.originPrice|currency}}</div>\n              <div class=\"course-card__price--discount\">{{course.price|currency}}</div>\n            </div>\n            <add-to-cart :course=\"course\"></add-to-cart>\n          </div>\n        </router-link>\n      </div>\n      <template v-if=\"showBtn\" slot=\"prev\">\n        <button class=\"carousel-btn carousel-btn--previous\">\n          <span class=\"prev\">\n            <i class=\"fas fa-angle-left\"></i>\n          </span>\n        </button>\n      </template>\n      <template v-if=\"showBtn\" slot=\"next\">\n        <button class=\"carousel-btn carousel-btn--next\">\n          <span class=\"next\">\n            <i class=\"fas fa-angle-right\"></i>\n          </span>\n        </button>\n      </template>\n    </carousel>\n  </div>\n</template>\n\n<script>\nimport carousel from \"vue-owl-carousel\";\nimport StarRating from \"vue-star-rating\";\nimport AddToCart from \"@/components/AddToCart\";\n\nexport default {\n  name: \"CourseCard\",\n  props: [\"courseCard\", \"selectedCat\"],\n  components: { carousel, StarRating, AddToCart },\n  data() {\n    return {\n      showBtn: false\n    };\n  },\n  computed: {\n    selectCat() {\n      return this.$store.state.courses.selectCat;\n    }\n  },\n\n  methods: {\n    courseTitle(title, len) {\n      if (title.length > len) {\n        title = title.substring(0, len - 1) + \"...\";\n      }\n      return title;\n    },\n    measureSize() {\n      let width = window.innerWidth;\n      let cardNum;\n\n      switch (true) {\n        case width < 520:\n          cardNum = 2;\n          break;\n        case width < 900:\n          cardNum = 2;\n          break;\n        case width < 1200:\n          cardNum = 3;\n          break;\n        case width < 1800:\n          cardNum = 4;\n          break;\n\n        case width > 1800:\n          cardNum = 5;\n          break;\n      }\n      this.showBtn = this.courseCard.length <= cardNum ? false : true;\n    }\n  },\n  created() {\n    window.addEventListener(\"resize\", this.measureSize);\n    this.measureSize();\n  }\n};\n</script>\n"]}]}