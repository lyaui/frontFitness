\" :src=\"course.imageUrl\" :alt=\"course.title\">\n            <router-link\n              class=\"cart__link d-block-phone\"\n              :to=\"{name:'CourseDetails',params:{course_name:course.title}}\"\n            >\n              <div class=\"cart__tag-box\">\n                <div v-if=\"course.discount\" class=\"cart__tag\">精選課程</div>\n              </div>\n\n              <div class=\"cart__title\">{{course.title}}</div>\n            </router-link>\n          </div>\n\n          <div class=\"cart__info\">\n            <router-link\n              class=\"cart__link\"\n              :to=\"{name:'CourseDetails',params:{course_name:course.title}}\"\n            >\n              <div class=\"cart__tag-box d-none-phone\">\n                <div v-if=\"course.discount\" class=\"cart__tag\">精選課程</div>\n              </div>\n\n              <div class=\"cart__title d-none-phone\">{{course.title}}</div>\n              <div class=\"d-flex\">\n                <div class=\"cart__category\">{{course.categorySelected}}｜</div>\n                <div class=\"cart__coach\">{{course.coach}}｜</div>\n                <div class=\"cart__time\">{{course.week}} {{course.time}}</div>\n              </div>\n            </router-link>\n            <div class=\"cart__handle\">\n              <div class=\"cart__price\">\n                <div class=\"cart__price--origin\">{{course.price|currency}}</div>\n              </div>\n              <button class=\"cart__remove btn\" @click=\"$store.commit('removeItem',course)\">\n                <i class=\"far fa-trash-alt\"></i>\n              </button>\n            </div>\n          </div>\n        </li>\n      </ul>\n      <div class=\"modal-buttons\">\n        <button\n          @click=\"checkout()\"\n          class=\"swal2-confirm swal2-styled\"\n          style=\"display: inline-block; background-color: rgb(255, 87, 33); border-left-color: rgb(255, 87, 33); border-right-color: rgb(255, 87, 33);\"\n        >結帳</button>\n\n        <button class=\"swal2-cancel swal2-styled\" @click=\"hide()\">繼續購物</button>\n      </div>\n    </div>\n  </modal>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {};\n  },\n  methods: {\n    checkout() {\n      this.hide();\n      this.$router.push(\"/cart\");\n    },\n    hide() {\n      this.$emit(\"child\");\n      this.$modal.hide(\"addtocart\");\n    }\n  }\n};\n</script>"]}]}