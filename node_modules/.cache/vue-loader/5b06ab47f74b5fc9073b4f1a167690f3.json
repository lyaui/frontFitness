 class=\"course-list__header\">\n        <div class=\"course-list__hot-tag\" v-if=\"course.discount\">\n          <div class=\"course-list__hot-tag-text\">精選</div>\n        </div>\n        <img class=\"course-list__img\" :src=\"course.imageUrl\" alt />\n      </div>\n\n      <div class=\"course-list__content\">\n        <div class=\"course-list__info\">\n          <div class=\"course-list__title heading-tertiary--dark\">\n            {{ textLimit(course.title, 20) }}\n          </div>\n          <div class=\"d-flex d-none-phone\">\n            <div class=\"course-list__category\">\n              {{ course.categorySelected }}｜\n            </div>\n            <div class=\"course-list__coach\">{{ course.coach }}｜</div>\n            <div class=\"course-list__time\">\n              {{ course.week }} {{ course.time }}\n            </div>\n          </div>\n\n          <div class=\"course-list__time\"></div>\n          <div class=\"course-list__intro d-none-phone\">\n            {{ textLimit(course.intro, 50) }}\n          </div>\n        </div>\n\n        <div class=\"course-list__price\">\n          <div class=\"course-list__price-info\">\n            <div class=\"course-list__price--discount\">\n              {{ course.price | currency }}\n            </div>\n            <div class=\"course-list__price--origin\">\n              {{ course.originPrice | currency }}\n            </div>\n\n            <star-rating\n              :rating=\"course.avgRating\"\n              :increment=\"0.1\"\n              :star-size=\"17\"\n              :read-only=\"true\"\n              style=\"margin-left:auto\"\n            ></star-rating>\n          </div>\n          <add-to-cart\n            class=\"d-none-tab-p\"\n            style=\"width:140px;margin-top:auto\"\n            :course=\"course\"\n          ></add-to-cart>\n        </div>\n      </div>\n    </router-link>\n  </div>\n</template>\n\n<script>\nimport StarRating from 'vue-star-rating';\nimport AddToCart from '@/components/AddToCart';\n\nexport default {\n  name: 'CourseList',\n  props: ['coursesList'],\n  components: { StarRating, AddToCart },\n  computed: {\n    remainCourse() {\n      return this.coursesList.filter((course) => course.remainQuantity > 0);\n    },\n  },\n  methods: {\n    textLimit(title, len) {\n      if (title.length > len) {\n        title = `${title.substring(0, len - 1)}...`;\n      }\n      return title;\n    },\n  },\n};\n</script>\n"]}]}