\\n    loadMore() {\\n      let arrayShow = this.courses;\\n      if (this.infiniteData.length <= arrayShow.length) {\\n        this.busy = true;\\n\\n        setTimeout(() => {\\n          for (var i = 0, j = 5; i < j; i++) {\\n            if (this.count === arrayShow.length) {\\n              break;\\n            }\\n            this.infiniteData.push(arrayShow[this.count++]);\\n          }\\n          this.busy = false;\\n        }, 500);\\n      }\\n    },\\n\\n    // searchCourse() {\\n    //   if (this.searchWord) {\\n    //     this.keyWord = this.searchWord;\\n    //     this.reset();\\n    //     this.searchShow = true;\\n    //     db.collection(\\\"courses\\\")\\n    //       .get()\\n    //       .then(snapshot => {\\n    //         snapshot.forEach(doc => {\\n    //           let course = doc.data();\\n    //           course.id = doc.id;\\n    //           this.courses.push(course);\\n    //         });\\n    //       })\\n    //       .then(() => {\\n    //         this.courses =\\n    //           this.courses.filter(course => course.title.match(this.keyWord)) ||\\n    //           [];\\n    //         this.loadMore();\\n    //       });\\n    //   }\\n    // },\\n    orderCourse(property, order) {\\n      this.courses = order\\n        ? this.courses.sort((a, b) => b[property] - a[property])\\n        : this.courses.sort((a, b) => a[property] - b[property]);\\n      this.count = 0;\\n      this.infiniteData = [];\\n      this.loadMore();\\n    }\\n  },\\n\\n  created() {\\n    this.reset();\\n    if (this.$store.state.courses.courses.length===[]) {\\n      this.$store.dispatch(\\\"courses/getCourses\\\");\\n    }\\n  },\\n\\n  //scroll\\n  mounted() {\\n    window.addEventListener(\\\"scroll\\\", this.onScroll);\\n  },\\n  beforeDestroy() {\\n    window.removeEventListener(\\\"scroll\\\", this.onScroll);\\n  }\\n};\\n</script>\\n\",\"import mod from \\\"-!../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../node_modules/thread-loader/dist/cj