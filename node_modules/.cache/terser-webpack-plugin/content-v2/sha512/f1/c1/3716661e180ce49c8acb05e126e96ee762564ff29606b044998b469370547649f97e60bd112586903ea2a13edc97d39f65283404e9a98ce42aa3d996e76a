);\\n      if (this.isRegisterValid) {\\n        this.$store\\n          .dispatch(\\\"auth/signUp\\\", this.form)\\n          .then(async user => {\\n            await this.$store.dispatch(\\\"auth/createUserProfile\\\", {\\n              uid: user.uid,\\n              userProfile: {\\n                userId: user.uid,\\n                name: this.form.name,\\n                email: this.form.email,\\n                userImg: this.form.img\\n              }\\n            });\\n            this.$toasted.success(\\\"\\\", {\\n              duration: 2000\\n            });\\n            this.hide();\\n          })\\n          .catch(error => this.$toasted.error(error, { duration: 3000 }));\\n      } else {\\n        this.$toasted.error(\\\"\\\", { duration: 3000 });\\n      }\\n    },\\n\\n    //\\n    handleLogin() {\\n      this.$v.$touch();\\n      if (this.isLoginValid) {\\n        this.$store\\n          .dispatch(\\\"auth/signIn\\\", this.form)\\n          .then(_ => {\\n            this.$toasted.success(\\\"\\\", {\\n              duration: 2000\\n            });\\n            this.hide();\\n          })\\n          .catch(error => this.$toasted.error(error, { duration: 2000 }));\\n      } else {\\n        this.$toasted.error(\\\"\\\", { duration: 3000 });\\n      }\\n    },\\n\\n    //\\n    logOut() {\\n      this.$store.dispatch(\\\"auth/signOut\\\").then(_ => {\\n        this.$toasted.success(\\\"\\\", {\\n          duration: 2000\\n        });\\n        this.$router.push({ name: \\\"home\\\" });\\n      });\\n    },\\n\\n    //\\n    show(status) {\\n      this.$modal.show(\\\"logRegModal\\\");\\n    },\\n    hide() {\\n      this.reset();\\n      this.$modal.hide(\\\"logRegModal\\\");\\n    }\\n  }\\n};\\n</script>\\n\\n<style lang=\\\"scss\\\">\\n</style>\",\"import mod from \\\"-!../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../node_modules/thread-loader/dist/cjs.js!../../node_modules/babel-loader/lib/index.js!../../node_modules/cache-loader