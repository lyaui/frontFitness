ll,\\n        toId: null\\n      },\\n      feedback: null,\\n      inputstatus: \\\"新留言\\\",\\n      inputShow: false\\n    };\\n  },\\n  props: [\\\"courseId\\\"],\\n  firestore() {\\n    return {\\n      //更新課程評價時使用\\n      course: db.collection(\\\"courses\\\").doc(this.courseId),\\n      //更新評價時使用\\n      comments: db.collection(\\\"comments\\\"),\\n      commentTo: {\\n        ref: db.collection(\\\"comments\\\").where(\\\"toId\\\", \\\"==\\\", this.courseId),\\n        resolve: data => {\\n          data.forEach(doc => this.ratings.push(doc.rating));\\n          //計算初始平均評價\\n          this.avgRating();\\n        }\\n      }\\n    };\\n  },\\n  components: {\\n    StarRating\\n  },\\n  methods: {\\n    addComment() {\\n      if (this.comment.rating) {\\n        //新增評價分數\\n        this.ratings.push(this.comment.rating);\\n        //計算平均評價\\n        this.avgRating();\\n        this.feedback = null;\\n        this.comment.user = this.user.userId;\\n        this.comment.timestamp = moment(Date.now()).format(\\\"l\\\");\\n        this.comment.toId = this.courseId;\\n        //新增留言\\n        this.$firestore.comments.add(this.comment);\\n        //更新評價分數\\n        this.updateAvgRating();\\n        Swal.fire({\\n          type: \\\"success\\\",\\n          title: `已收到您的評價！`,\\n          showConfirmButton: false,\\n          timer: 1500\\n        });\\n      } else {\\n        this.feedback = \\\"請選取課程評價分數再送出\\\";\\n      }\\n    },\\n    updateComment(comment) {\\n      this.comment.timestamp = moment(Date.now()).format(\\\"l\\\");\\n      this.$firestore.comments.doc(comment.id).update({\\n        comment: comment.comment,\\n        rating: comment.rating\\n      });\\n      this.updateAvgRating();\\n      Swal.fire({\\n        type: \\\"success\\\",\\n        title: `已收到您的評價！`,\\n        showConfirmButton: false,\\n        timer: 1500\\n      });\\n      this.inputShow = !this.inputShow;\\n    },\\n    avgRating() {\\n      if (this.ratings.length === 0) {\\n        this.avgStarRating = 0;\\n      } else {\\n        this.avgStarRating = parseFloat(\\n          (\\n            this.ratings.reduce((acc, cur) => cur + acc) / this.ratings.length\\n          ).toFixed(1)\\n        );\\n      }\\n      //更新progressbar\\n      this.progressBar();\\n    },\\n    progressBar() {\\n      this.ratingsSorted = this.ratingsSorted\\n        .map((num, index) => this.ratings.filter(el => el === index + 1).length)\\n        .reverse();\\n      this.ratingsSorted = this.ratingsSorted.map(el =>\\n        this.ratings.length === 0\\n          ? 0\\n          : ((el / this.ratings.length) * 100).toFixed(0)\\n      );\\n    },\\n\\n    updateAvgRating() {\\n      this.$firestore.course.update({\\n        avgRating: this.avgStarRating\\n      });\\n    },\\n    checkUserComment() {\\n      this.commentTo.forEach(comment => {\\n        if (comment.user === this.user.userId) {\\n          this.comment = comment;\\n          this.inputShow = true;\\n        }\\n      });\\n    }\\n  },\\n\\n  created() {\\n    let user = fb.auth().currentUser;\\n    if (!user) {\\n      this.user = \\\"無使用者\\\";\\n    }\\n    db.collection(\\\"users\\\")\\n      .get()\\n      .then(snapshot => {\\n        snapshot.forEach(doc => {\\n          let id = doc.data().userId;\\n          let name = doc.data().name;\\n          let img = doc.data().userImg;\\n          //確認current user身分\\n          if (user && id === user.uid) {\\n            this.user = doc.data();\\n          }\\n          //確認每位留言者身分\\n          this.commentTo.forEach(comment => {\\n            if (id === comment.user) {\\n              comment.img = img;\\n              comment.name = name;\\n            }\\n          });\\n        });\\n        this.comments = this.commentTo;\\n        this.checkUserComment();\\n      });\\n  }\\n};\\n</script>\",\"import mod from \\\"-!../../node_modules/cache-loader/dist/cjs.js??ref--12-0!../../node_modules/thread-loader/dist/cjs.js!../../node_modules/babel-loader/l