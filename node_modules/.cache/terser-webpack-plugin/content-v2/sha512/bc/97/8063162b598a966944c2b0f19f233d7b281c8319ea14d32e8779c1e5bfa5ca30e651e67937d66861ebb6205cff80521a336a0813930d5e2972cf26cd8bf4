\\n              <button\\n                v-if=\\\"status==='註冊'\\\"\\n                @click=\\\"signup()\\\"\\n                class=\\\"swal2-confirm swal2-styled\\\"\\n                style=\\\"display: inline-block; background-color: rgb(255, 87, 33); border-left-color: rgb(255, 87, 33); border-right-color: rgb(255, 87, 33);\\\"\\n              >註冊</button>\\n              <button\\n                v-if=\\\"status==='登入'\\\"\\n                @click=\\\"login()\\\"\\n                class=\\\"swal2-confirm swal2-styled\\\"\\n                style=\\\"display: inline-block; background-color: rgb(255, 87, 33); border-left-color: rgb(255, 87, 33); border-right-color: rgb(255, 87, 33);\\\"\\n              >登入</button>\\n              <button class=\\\"swal2-cancel swal2-styled\\\" @click=\\\"hide\\\">取消</button>\\n              <button class=\\\"btn\\\"></button>\\n            </div>\\n          </form>\\n        </div>\\n      </div>\\n    </modal>\\n  </nav>\\n</template>\\n\\n<script>\\nimport $ from \\\"jquery\\\";\\nimport { fb, db } from \\\"@/firebase/init\\\";\\nimport Swal from \\\"sweetalert2\\\";\\nimport moment from \\\"moment\\\";\\n\\nexport default {\\n  data() {\\n    return {\\n      search: null,\\n      user: [],\\n      email: \\\"\\\",\\n      password: \\\"\\\",\\n      name: \\\"\\\",\\n      timestamp:\\\"\\\",\\n      feedback: \\\"\\\",\\n      status: \\\"\\\",\\n      img: \\\"https://png2.pngtree.com/svg/20161215/9bc7bae28b.png\\\"\\n    };\\n  },\\n\\n  methods: {\\n    sendSearch() {\\n      if (this.search) {\\n        this.$router.push({ name: \\\"courses-index\\\" });\\n        this.$emit(\\\"searchInput\\\", this.search);\\n        this.search = null;\\n        \\n      }\\n    },\\n    signup() {\\n      if (this.name && this.email && this.password) {\\n        fb.auth()\\n          .createUserWithEmailAndPassword(this.email, this.password)\\n          .then(user => {\\n            db.collection(\\\"users\\\")\\n              .d