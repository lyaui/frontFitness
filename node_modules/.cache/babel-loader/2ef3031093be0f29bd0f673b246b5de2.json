\n\n<script>\nimport { fb, db } from \"@/firebase/init\";\nimport Pagination from \"@/components/Pagination\";\nimport moment from \"moment\";\n\nexport default {\n  components: { Pagination },\n  data() {\n    return {\n      payments: []\n    };\n  },\n  methods: {\n    time(num) {\n      return moment(num).format(\"YYYY/MM/DD HH:mm\");\n    }\n  },\n  created() {\n    this.paymentId = this.$route.params.paymentId;\n    db.collection(\"payments\")\n      .orderBy(\"timestamp\", \"desc\")\n      .get()\n      .then(snapshot =>\n        snapshot.forEach(doc => this.payments.push(doc.data()))\n      );\n  }\n};\n</script>\n"],"sourceRoot":"src/views"}]}