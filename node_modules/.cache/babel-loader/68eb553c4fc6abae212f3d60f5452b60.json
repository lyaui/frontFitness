A,CAAA,MAAA,GAAA,GAAA,EAAA;AACA,QAAA,KAAA,GAAA,KAAA,CAAA,SAAA,CAAA,CAAA,EAAA,GAAA,GAAA,CAAA,IAAA,KAAA;AACA;;AACA,aAAA,KAAA;AACA;AAPA,GAPA;AAgBA,EAAA,UAAA,EAAA;AAAA,IAAA,QAAA,EAAA,QAAA;AAAA,IAAA,UAAA,EAAA;AAAA,GAhBA;AAiBA,EAAA,OAjBA,qBAiBA,CAAA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"course-carousel mb-sm\" >\n    <carousel\n      :nav=\"false\"\n      v-if=\"courseCard.length>0\"\n      :margin=\"30\"\n      :dots=\"false\"\n      :rewind=\"false\"\n      :responsive=\"{300:{items:1,stagePadding:30,margin:80},450:{items:1,stagePadding:50,margin:80},520:{items:2,margin:30},750:{items:2,stagePadding:30,margin:60},900:{items:3},1200:{items:4},1600:{items:5}}\"\n    >\n      <template slot=\"prev\">\n        <button class=\"carousel-btn carousel-btn--previous\">\n          <span class=\"prev\">\n            <i class=\"fas fa-angle-left\"></i>\n          </span>\n        </button>\n      </template>\n\n      <div class=\"course-card\" v-for=\"course in courseCard\" v-if=\"course.remainQuantity>0&&course.discount\" >\n        <router-link :to=\"{name:'CourseDetails',params:{course_name:course.title}}\">\n          <div class=\"course-card__header\">\n            <div class=\"course-card__hot-tag\" v-if=\"course.discount\">\n              <div class=\"course-card__hot-tag-text\">精選</div>\n            </div>\n            <img class=\"course-card__img\" :src=\"course.imageUrl\" alt>\n          </div>\n          <div class=\"course-card__body\">\n            <div class=\"course-card__title heading-tertiary--dark\">{{courseTitle(course.title,20)}}</div>\n            <div class=\"d-flex\">\n              <div class=\"course-card__category\">{{course.categorySelected}}｜</div>\n              <div class=\"course-card__coach\">{{course.coach}}</div>\n            </div>\n            <div class=\"course-card__rating\">\n              <star-rating :rating=\"course.avgRating\" :increment=\"0.1\" :star-size=\"18\" :read-only=\"true\"></star-rating>\n            </div>\n            <div class=\"course-card__price\">\n              <div class=\"course-card__price--origin\">{{course.originPrice|currency}}</div>\n              <div class=\"course-card__price--discount\">{{course.price|currency}}</div>\n            </div>\n            <AddToCart :courseCard=\"course\"></AddToCart>\n          </div>\n        </router-link>\n      </div>\n      <template slot=\"next\">\n        <button class=\"carousel-btn carousel-btn--next\">\n          <span class=\"next\">\n            <i class=\"fas fa-angle-right\"></i>\n          </span>\n        </button>\n      </template>\n    </carousel>\n  </div>\n</template>\n\n<script>\nimport carousel from \"vue-owl-carousel\";\nimport StarRating from \"vue-star-rating\";\n\nexport default {\n  props: [\"courseCard\"],\n  data() {\n    return {\n      rating: 5\n    };\n  },\n  methods: {\n    //標題不超過20字元\n    courseTitle(title,len) {\n      if (title.length > len) {\n        title = title.substring(0, len - 1) + \"...\";\n      }\n      return title;\n    },\n  },\n  components: { carousel, StarRating },\n  created() {}\n};\n</script>"],"sourceRoot":"src/components"}]}