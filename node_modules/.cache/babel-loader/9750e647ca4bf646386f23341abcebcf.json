tar-rating\";\n\nexport default {\n  props: [\"courseCard\"],\n  data() {\n    return {\n      rating: 5\n    };\n  },\n  methods: {\n    //標題不超過20字元\n    courseTitle(title) {\n      let len = 20;\n      if (title.length > len) {\n        title = title.substring(0, len - 1) + \"...\";\n      }\n      return title;\n    }\n  },\n  components: { carousel, StarRating }\n};\n</script>"],"sourceRoot":"src/components"}]}