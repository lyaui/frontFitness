n            <div class=\"course-recommend__coach\">{{ course.coach }}｜</div>\n            <div class=\"course-recommend__time\">\n              {{ course.week }} {{ course.time }}\n            </div>\n          </div>\n\n          <div class=\"course-recommend__time\"></div>\n        </div>\n        <div class=\"d-flex\">\n          <div class=\"course-recommend__price\">\n            <star-rating\n              :rating=\"course.avgRating\"\n              :increment=\"0.1\"\n              :star-size=\"18\"\n              :read-only=\"true\"\n            ></star-rating>\n          </div>\n          <div class=\"course-recommend__price\">\n            <div class=\"course-recommend__price--discount\">\n              {{ course.price | currency }}\n            </div>\n            <div class=\"course-recommend__price--origin\">\n              {{ course.originPrice | currency }}\n            </div>\n          </div>\n        </div>\n      </div>\n    </router-link>\n  </div>\n</template>\n\n<script>\nimport StarRating from 'vue-star-rating';\n\nexport default {\n  name: 'CourseRecommend',\n  props: ['category', 'courseId'],\n  components: {\n    StarRating,\n  },\n  computed: {\n    courses() {\n      return this.$store.state.courses.courses.filter(\n        (course) => course.categorySelected === this.category\n          && course.id !== this.courseId\n          && course.remainQuantity > 0,\n      );\n    },\n  },\n  methods: {\n    courseTitle(title) {\n      const len = 20;\n      if (title.length > len) {\n        title = `${title.substring(0, len - 1)}...`;\n      }\n      return title;\n    },\n  },\n  created() {\n    if (this.$store.state.courses.courses === []) {\n      this.$store.dispatch('courses/getCourses');\n    }\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}