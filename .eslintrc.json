{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-console": "off",
    "no-param-reassign": 0,
    "global-require": 0,
    "max-len": [
      "error",
      {
        "code": 150
      }
    ],
    "object-curly-newline": "off",
    "no-plusplus": "off",
    "import/no-unresolved": [2, { "caseSensitive": false }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["@"]
      }
    }
  }
}
